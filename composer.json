{
    "name": "axn/tool-kit-for-laravel",
    "description": "Includes a set of useful tools for the Laravel framework.",
    "homepage": "https://github.com/AXN-Informatique/laravel-extension",
    "support": {
        "issues": "https://github.com/AXN-Informatique/laravel-extension/issues"
    },
    "authors": [
        {
            "name": "AXN Informatique",
            "email": "developpement@axn.fr"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "composer/semver": "^3.0.0",
        "forxer/generic-term-translations-for-laravel": "^1.6.0",
        "laravel/framework": "^10.0 || ^11.0"
    },
    "require-dev": {
        "laravel/pint": "^1.16.0",
        "driftingly/rector-laravel": "^1.2",
        "rector/rector": "^1.1.0"
    },
    "autoload": {
        "psr-4": {
            "Axn\\ToolKit\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Axn\\ToolKit\\ServiceProvider"
            ]
        }
    }
}
